
{
  "manifest_version": 2,
  "name": "Airliners.net Large Photo View",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.4.min.js","content.js"]
    }
  ],
  "background": {
  "scripts": ["background.js"]
},
  "browser_action": {
    "default_icon": "icon.png"
  },
    "permissions": [
        "activeTab",
        "tabs"
    ]
}